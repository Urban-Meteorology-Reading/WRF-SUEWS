                " REAL,OPTIONAL,DIMENSION(ims:ime,7,jms:jme),INTENT(INOUT) :: landusef_SUEWS",
                " REAL,OPTIONAL,DIMENSION(ims:ime,7,jms:jme),INTENT(INOUT) :: alb_SUEWS",
                " REAL,OPTIONAL,DIMENSION(ims:ime,7,jms:jme),INTENT(INOUT) :: emis_SUEWS",
